{
  "name": "@dareharu/prettier-config",
  "version": "1.0.1",
  "description": "Shareable prettier configuration for Dareharu project",
  "author": "@dareharu",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "sideEffects": false,
  "homepage": "https://github.com/dareharu/shareable-configs/tree/main/packages/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dareharu/shareable-configs",
    "directory": "packages/prettier-config"
  },
  "bugs": {
    "url": "https://github.com/github.com/dareharu/shareable-configs/issues"
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^1.10.0",
    "@vitest/coverage-c8": "^0.30.1",
    "tsup": "^6.7.0",
    "vitest": "^0.30.1"
  },
  "keywords": [
    "dareharu",
    "prettier",
    "prettier config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ]
}
