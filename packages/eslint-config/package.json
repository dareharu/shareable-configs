{
  "name": "@dareharu/eslint-config",
  "version": "1.2.1",
  "description": "Shareable ESLint configuration for Dareharu project",
  "author": "@dareharu",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./common": {
      "import": "./dist/common.mjs",
      "require": "./dist/common.js",
      "types": "./dist/common.d.ts"
    },
    "./import": {
      "import": "./dist/import.mjs",
      "require": "./dist/import.js",
      "types": "./dist/import.d.ts"
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/dareharu/shareable-configs/tree/main/packages/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dareharu/shareable-configs",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/github.com/dareharu/shareable-configs/issues"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\"",
    "build": "tsup",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "dependencies": {},
  "devDependencies": {
    "@favware/cliff-jumper": "^4.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "tsup": "^8.2.4",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  },
  "optionalDependencies": {
    "eslint-plugin-import": "^2.30.0"
  },
  "keywords": [
    "dareharu",
    "eslint",
    "eslint-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ]
}
