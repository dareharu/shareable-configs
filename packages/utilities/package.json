{
  "name": "@dareharu/utilities",
  "version": "0.2.0",
  "description": "Common JavaScript utilities for Dareharu project",
  "author": "@dareharu",
  "license": "MIT",
  "main": "dist/lib.js",
  "module": "dist/lib.mjs",
  "types": "dist/lib.d.ts",
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    },
    "./createBitField": {
      "import": "./dist/lib/createBitField.mjs",
      "require": "./dist/lib/createBitField.js",
      "types": "./dist/lib/createBitField.d.ts"
    },
    "./keyMirror": {
      "import": "./dist/lib/keyMirror.mjs",
      "require": "./dist/lib/keyMirror.js",
      "types": "./dist/lib/keyMirror.d.ts"
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/dareharu/shareable-configs/tree/main/packages/utilities",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dareharu/shareable-configs",
    "directory": "packages/utilities"
  },
  "bugs": {
    "url": "https://github.com/github.com/dareharu/shareable-configs/issues"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\"",
    "build": "tsup",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "dependencies": {},
  "devDependencies": {
    "@favware/cliff-jumper": "^1.10.0",
    "@vitest/coverage-c8": "^0.33.0",
    "tsup": "^6.7.0",
    "vitest": "^0.34.1"
  },
  "keywords": [
    "dareharu",
    "typescript",
    "ts",
    "utilities",
    "useful",
    "valuable",
    "effective"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ]
}
